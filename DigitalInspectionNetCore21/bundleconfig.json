			"wwwroot/js/Library/DataTables/jquery.dataTables.min.js",
			"wwwroot/js/Library/DataTables/dataTables.select.min.js",
			"wwwroot/js/Library/Bootstrap-Material/material.js",
			"wwwroot/js/Library/Bootstrap-Material/ripples.js",
